  <%= csrf_meta_tags %>
  <%= render 'shared/shim' %>
</head>
<body>
  <div class="page">
    <div class="container wrap">
      <div class="header">
        <%= render 'shared/nav' %>
        <h3><%= link_to "Флэшкарточкер", root_path, class: "logo" %></h3>
      </div>
      <%= yield %>
    </div>
   <!-- <%= render 'shared/footer' %> -->
  </div>
</body>
</html>
